[buildout]
parts = walkman
versions = versions
develop = {{ pyo_path }}

[walkman]
recipe = zc.recipe.egg
eggs =
    pyo
    audiowalkman
    {% for module in module_dict.keys() %}
    {{ module }}
    {% endfor %}

[versions]
audiowalkman = {{ audiowalkman_version }}
{% for module, version in module_dict.items() %}
{{ module }} = {{ version }}
{% endfor %}
